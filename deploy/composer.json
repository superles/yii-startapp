{
    "name": "crisu83/yii-app",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/superles/yii-auth"
        },
        {
            "type": "vcs",
            "url": "https://github.com/superles/yiistrap"
        },
        {
            "type": "vcs",
            "url": "https://github.com/superles/yii-consoletools"
        },
        {
            "type": "vcs",
            "url": "https://github.com/superles/NordCms"
        },
        {
            "type": "vcs",
            "url": "https://github.com/superles/yii-user"
        }



    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",

        "crisu83/yii-composer": "dev-master",
        "mishamx/yii-user":"dev-master",
        "boo1ean/NordCms":"dev-master",
        "crisu83/yii-auth": "dev-master",
        "crisu83/yii-consoletools": "dev-master",
        "crisu83/yiistrap": "dev-bs3",

        "yiisoft/yii": ">=1.1.14",
        "twbs/bootstrap": "v3.1.1",
        "nordsoftware/yii-emailer": "1.2.2",
        "nordsoftware/yii-audit": "1.2.3"

    },
    "require-dev": {
        "codeception/codeception": "@stable",
        "malyshev/yii-debug-toolbar": "dev-master"
    },

    "scripts": {
        "post-install-cmd": [
            "Crisu83\\YiiComposer\\InstallHandler::setPermissions"
        ]
    },
    "extra": {
        "yii-install-writable": [
            "app/runtime",
            "app/tests/_log",
            "web/assets"
        ],
        "yii-install-executable": [
            "app/yiic"
        ]
    }
}
